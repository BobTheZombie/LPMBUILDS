name = "gcc-libs"
version = "14.2.0"
summary = "Runtime libraries shipped by GCC"
url = "https://gcc.gnu.org/"
release = "1"
arch = "x86_64"

requires = ['glibc']
provides = ['gcc-libs', 'libstdc++', 'libgcc_s']
conflicts = []
obsoletes = []

prepare() { :; }
build() { ./configure --prefix=/usr || true; make -j"$(nproc)" || true; }
install() { make DESTDIR="$PKGDIR" install || true; }

