name = "mpfr"
version = "4.2.1"
summary = "Multiple-precision floating-point"
url = "https://www.mpfr.org/"
release = "1"
arch = "x86_64"

requires = ['glibc', 'gmp']
provides = ['mpfr']
conflicts = []
