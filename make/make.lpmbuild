name = "make"
version = "4.4.1"
summary = "GNU make"
url = "https://www.gnu.org/software/make/"
release = "1"
arch = "x86_64"

requires = ['glibc']
provides = ['make']
conflicts = []
obsoletes = []

prepare() { :; }
build() { ./configure --prefix=/usr || true; make -j"$(nproc)" || true; }
install() { make DESTDIR="$PKGDIR" install || true; }

