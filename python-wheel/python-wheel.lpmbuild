name = "python-wheel"
version = "0.44.0"
summary = "Built-package format for Python"
url = "https://github.com/pypa/wheel"
release = "1"
arch = "x86_64"

requires = ['python', 'python-setuptools']
provides = ['python-wheel']
conflicts = []
obsoletes = []

prepare() { :; }
build() { ./configure --prefix=/usr || true; make -j"$(nproc)" || true; }
install() { make DESTDIR="$PKGDIR" install || true; }

