name = "pkgconf"
version = "2.3.0"
summary = "Package compiler and linker metadata toolkit"
url = "https://github.com/pkgconf/pkgconf"
release = "1"
arch = "x86_64"

requires = ['glibc']
provides = ['pkgconf']
conflicts = []
obsoletes = []

prepare() { :; }
build() { ./configure --prefix=/usr || true; make -j"$(nproc)" || true; }
install() { make DESTDIR="$PKGDIR" install || true; }

