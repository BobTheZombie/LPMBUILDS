name = "python-pip"
version = "24.2"
summary = "The PyPA installer"
url = "https://pip.pypa.io/"
release = "1"
arch = "x86_64"

requires = ['python', 'python-setuptools', 'python-wheel']
provides = ['python-pip']
conflicts = []
obsoletes = []

prepare() { :; }
build() { ./configure --prefix=/usr || true; make -j"$(nproc)" || true; }
install() { make DESTDIR="$PKGDIR" install || true; }

