name = "gmp"
version = "6.3.0"
summary = "GNU multiple precision arithmetic"
url = "https://gmplib.org/"
release = "1"
arch = "x86_64"

requires = ['glibc']
provides = ['gmp']
conflicts = []
obsoletes = []

prepare() { :; }
build() { ./configure --prefix=/usr || true; make -j"$(nproc)" || true; }
install() { make DESTDIR="$PKGDIR" install || true; }

